        <div class="container">

            <h2 class="text-bold grey-text font-40">PREÇOS</h2>

        </div>

    </section>

    <!--page title end-->







    <section class="section-padding">

        <div class="container">




            <div class="portfolio-container text-center">




            <ul class="precos">
                      <li><a href="http://admin.mrjackrj.com.br/aparelhos/buscar?IdMarca=26">Apple</a></li>
                      <li><a href="http://admin.mrjackrj.com.br/aparelhos/buscar?IdMarca=29" >Samsung</a></li>
                      <li><a href="http://admin.mrjackrj.com.br/aparelhos/buscar?IdMarca=31" >Sony</a></li>
                      <li><a href="http://admin.mrjackrj.com.br/aparelhos/buscar?IdMarca=32" >LG</a></li>
                      <li><a href="http://admin.mrjackrj.com.br/aparelhos/buscar?IdMarca=30" >Asus</a></li>
                      <li><a href="http://admin.mrjackrj.com.br/aparelhos/buscar?IdMarca=27" >Motorola</a></li>
                      <li><a href="http://admin.mrjackrj.com.br/aparelhos/buscar?IdMarca=28" >Nokia</a></li>
                  </ul>





                <div class="portfolio portfolio-with-title col-4 gutter mtb-50">

                    @if (Model.Count() > 0)

                    {

                        foreach (var item in Model)

                        {

                            <div class="portfolio-item" data-groups='["all", "branding", "photography"]' style="top:100px;">

                                <div class="portfolio-wrapper">



                                    <div class="thumb">

                                        <img src="http://admin.mrjackrj.com.br/ImgAparelhos/@item.modelo.PathImagem" alt="">

                                    </div><!-- thumb -->



                                    <div class="portfolio-title">

                                        <h2><a href="#">@item.modelo.Descricao</a></h2>

                                        <select name="dropdownmain">

                                            <option value="qualservico">Qual o Serviço</option>

                                            @foreach (var def in item.defeitos)

                                            {

                                                <option value="@def.Id">@def.Defeito</option>

                                            }

                                        </select>



                                        @foreach (var def in item.defeitos)

                                        {

                                            <input name="@def.Id" value="Pg Dinheiro: R$ @def.Dinheiro" disabled>

                                            <input name="@def.Id" value="Pg Cartão: R$ @def.Cartao" disabled>

                                        }

                                        <input name="qualservico" value="Pg Dinheiro:" disabled>

                                        <input name="qualservico" value="Pg Cartão:" disabled>

                                    </div>



                                </div><!-- /.portfolio-wrapper -->

                            </div><!-- /.portfolio-item -->

                        }

                    }



                </div><!-- /.portfolio-wrapper -->

