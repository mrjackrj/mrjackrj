    <script>

        $('input[name!="qualservico"]').hide();

        $('select[name="' + $('select[name="dropdownmain"]').val() + '"]').show();

        $('select[name="dropdownmain"]').change(function () {

            $('input[name!="dropdownmain"]').hide();

            $('input[name="' + $(this).val() + '"]').show();

        });

    </script>



