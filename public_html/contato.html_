        <script type="text/javascript">
function hgsubmit()
{
if (/\S+/.test(document.hgmailer.nome.value) == false) 
alert ("Por favor, digite um nome.");

else if (/^\S+@[a-z0-9_.-]+\.[a-z]{2,6}$/i.test(document.hgmailer.email1.value) == false) 
alert ("Um endereço de e-mail válido é requerido.");

else if (/\S+/.test(document.hgmailer.telefone.value) == false) 
alert ("Um telefone válido é requerido.");

else if (/\S+/.test(document.hgmailer.mensagem.value) == false) 
alert ("Por favor, digite sua mensagem..");
 
else {
document.hgmailer.submit();
alert ('Muito obrigado pela sua mensagem, em breve entraremos em contato..');
}
}

</script>


